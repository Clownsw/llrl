(assert-eq? "123" (string '123))
(assert-eq? "-728" (string '-728))
(assert-eq? "3.14" (string '3.14))
(assert-eq? "3.14" (string '3.14))
(assert-eq? "#t" (string '#t))
(assert-eq? "#f" (string '#f))
(assert-eq? "foo" (string 'foo))
(assert-eq? "option/unwrap*" (string 'option/unwrap*))
(assert-eq? "\"Hello, World!\\n\"" (string '"Hello, World!\n"))
(assert-eq? "#\\a" (string '#\a))
(assert-eq? "#\\\\n" (string '#\\n))
(assert-eq? "(a . b)" (string '(a . b)))
(assert-eq? "(a b . c)" (string '(a b . c)))
(assert-eq? "(a b c)" (string '(a b c)))
(assert-eq? "((a) (b) (c))" (string '((a) (b) (c))))
(assert-eq? "()" (string '()))
(assert-eq? "<use>" (string \string))

(assert-eq? "'foo" (string ''foo))
(assert-eq? "`(foo ,bar ,@(baz 'qux))" (string '`(foo ,bar ,@(baz 'qux))))
(assert-eq? "(set! i (+ ~i 1))" (string '(set! i (+ ~i 1))))
(assert-eq? "\\string" (string '\string))
(assert-eq? "((f a)? (g b)!)" (string '((f a)? (g b)!)))
(assert-eq? "(function f {I32})" (string '(function f {I32})))
(assert-eq? "(function (id x) {(forall A) (-> A A)} x)" (string '(function (id x) {(forall A) (-> A A)} x)))
