(import "std/ordered-map" _)
(import "std/tests/hash" h)

(assert (ordered-map/empty? ordered-map/empty))
(assert-eq? 0 (ordered-map/length ordered-map/empty))
(assert-eq? none (ordered-map/get? unit ordered-map/empty))
(assert (not (ordered-map/has? unit ordered-map/empty)))

(let1 tree ordered-map/empty
  (assert-eq? none (ordered-map/insert! unit #t tree))
  (assert (not (ordered-map/empty? tree)))
  (assert-eq? 1 (ordered-map/length tree))
  (assert-eq? (some #t) (ordered-map/get? unit tree))
  (assert (ordered-map/has? unit tree))
  (assert-eq? (some #t) (ordered-map/insert! unit #f tree))
  (assert-eq? (some #f) (ordered-map/get? unit tree)))

(let1 tree (ordered-map (: (some #t) #f) (: none #t))
  (assert-eq? (some #t) (ordered-map/get? none tree))
  (assert-eq? (some #f) (ordered-map/get? (some #t) tree))
  (assert-eq? none (ordered-map/get? (some #f) tree))
  (assert-eq? (some #t) (ordered-map/remove! none tree))
  (assert-eq? none (ordered-map/remove! none tree))
  (assert-eq? none (ordered-map/get? none tree)))

(let1 tree (ordered-map (: (some #t) #f) (: none #t))
  (ordered-map/clear! tree)
  (assert-eq? none (ordered-map/get? none tree))
  (assert-eq? none (ordered-map/get? (some #t) tree)))

(let1 tree (ordered-map/collect (array (: 1 "one") (: 2 "two") (: 3 "three")))
  (assert-eq? (some "one") (ordered-map/get? 1 tree))
  (assert-eq? (some "two") (ordered-map/get? 2 tree))
  (assert-eq? (some "three") (ordered-map/get? 3 tree))
  (assert-eq? none (ordered-map/get? 4 tree)))

(let1 tree (ordered-map (: 1 "A") (: 3 "B") (: 2 "C"))
  (assert-eq? (array (: 1 "A") (: 2 "C") (: 3 "B")) (collect (ordered-map/elems tree)))
  (assert-eq? (array 1 2 3) (collect (ordered-map/keys tree)))
  (assert-eq? (array "A" "C" "B") (collect (ordered-map/values tree)))
  (assert-eq? (some (: 1 "A")) (ordered-map/get-min? tree))
  (assert-eq? (some (: 3 "B")) (ordered-map/get-max? tree))
  (assert-eq? (some (: 1 "A")) (ordered-map/remove-min! tree))
  (assert-eq? (some (: 3 "B")) (ordered-map/remove-max! tree))
  (assert-eq? (some (: 2 "C")) (ordered-map/get-min? tree))
  (assert-eq? (some (: 2 "C")) (ordered-map/get-max? tree))
  (assert-eq? (some (: 2 "C")) (ordered-map/remove-min! tree))
  (assert-eq? none (ordered-map/remove-max! tree))
  (assert-eq? none (ordered-map/get-min? tree)))

(function size 1000)

(function patterns
  (array
    [^0 (iterator/range 0 size 1)]
    [^0 (it/map [^1 (- size 1 %1)] (iterator/range 0 size 1))]
    [^0 (it/map [^1 (% (* %1 3) size)] (iterator/range 0 size 1))]
    [^0 (it/map [^1 (% (* %1 7) size)] (iterator/range 0 size 1))]
    [^0 (it/map [^1 (% (* %1 41) size)] (iterator/range 0 size 1))]))

(for (let p) patterns
  (let1 tree ordered-map/empty
    (for (let i) (p)
      (ordered-map/insert! i (* i 2) tree))
    (assert-eq?
      (|> (iterator/range 0 size 1) (it/map [^1 (: %1 (* %1 2))]) collect)
      (collect (ordered-map/elems tree)) {(Array (: I32 I32))})))

(for (: (let i) (let p)) (it/with-index patterns)
  (for (: (let j) (let q)) (it/with-index patterns)
    (let ([tree ordered-map/empty]
          [exclude-index (+ (* (cast i) 10) (cast j))])
      (for (let i) (it/filter [^1 (ne? %1 exclude-index)] (p))
        (assert-eq? (ordered-map/insert! i (* i 2) tree) none))
      (for (let i) (it/filter [^1 (ne? %1 exclude-index)] (q))
        (assert-eq? (ordered-map/insert! i (* i 3) tree) (some (* i 2))))
      (for (let i) (q)
        (assert-eq? (ordered-map/remove! i tree) (if (eq? i exclude-index) none (some (* i 3)))))
      (assert (ordered-map/empty? tree)))))

(assert (ordered-map/empty? default))

(assert-eq? (string (debug (ordered-map (: 1 2) (: 3 4) (: 7 8) (: 5 6))))
            "(ordered-map (: 1 2) (: 3 4) (: 5 6) (: 7 8))")

(let* ([a (ordered-map (: 0 (ref 1)) (: 2 (ref 3)))]
       [b (shallow-copy a)]
       [c (deep-copy a)])
  (ordered-map/insert! 4 (ref 5) a)
  (ordered-map/insert! 6 (ref 7) b)
  (ordered-map/insert! 8 (ref 9) c)
  (set! (option/force (ordered-map/get? 0 a)) 11)
  (set! (option/force (ordered-map/get? 2 b)) 22)
  (set! (option/force (ordered-map/get? 0 c)) 33)
  (assert-eq? a (ordered-map (: 0 (ref 11)) (: 2 (ref 22)) (: 4 (ref 5))))
  (assert-eq? b (ordered-map (: 0 (ref 11)) (: 2 (ref 22)) (: 6 (ref 7))))
  (assert-eq? c (ordered-map (: 0 (ref 33)) (: 2 (ref 3)) (: 8 (ref 9)))))

(assert-eq? (ordered-map) (ordered-map))
(assert-ne? (ordered-map) (ordered-map (: 1 0)))
(assert-eq? (ordered-map (: 1 0)) (ordered-map (: 1 0)))
(assert-ne? (ordered-map (: 1 0)) (ordered-map (: 1 1)))
(assert-ne? (ordered-map (: 1 0)) (ordered-map (: 0 0)))
(assert-ne? (ordered-map (: 1 0)) (ordered-map (: 0 1)))
(assert-ne? (ordered-map (: 1 0)) (ordered-map (: 1 0) (: 2 0)))
(assert-ne? (ordered-map (: 2 0)) (ordered-map (: 1 0) (: 2 0)))

(assert (not (< (ordered-map) (ordered-map))))
(assert (< (ordered-map) (ordered-map (: 0 0))))
(assert (< (ordered-map (: 0 0)) (ordered-map (: 0 1))))
(assert (< (ordered-map (: 0 0)) (ordered-map (: 1 0))))
(assert (< (ordered-map (: 0 0) (: 1 1)) (ordered-map (: 1 0))))
(assert (< (ordered-map (: 0 0) (: 1 1)) (ordered-map (: 0 1))))
(assert (not (< (ordered-map (: 0 0) (: 1 1)) (ordered-map (: 0 0)))))
(assert (not (< (ordered-map (: 0 0) (: 1 1)) (ordered-map (: 0 0) (: 1 0)))))
(assert (< (ordered-map (: 0 0) (: 1 1)) (ordered-map (: 0 0) (: 1 2))))

(assert-eq? (h (ordered-map)) (h (ordered-map)))
(assert-ne? (h (ordered-map)) (h (ordered-map (: 1 0))))
(assert-eq? (h (ordered-map (: 1 0))) (h (ordered-map (: 1 0))))
(assert-ne? (h (ordered-map (: 1 0))) (h (ordered-map (: 1 1))))
(assert-ne? (h (ordered-map (: 1 0))) (h (ordered-map (: 0 0))))
(assert-ne? (h (ordered-map (: 1 0))) (h (ordered-map (: 0 1))))
(assert-ne? (h (ordered-map (: 1 0))) (h (ordered-map (: 1 0) (: 2 0))))
(assert-ne? (h (ordered-map (: 2 0))) (h (ordered-map (: 1 0) (: 2 0))))
