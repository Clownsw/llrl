(no-implicit-std)

(import "std/prelude/stage-0" _)
(import "std/assert" assert)
(import "std/num-literals" I32 F64)
(export exit time)

(function (exit status) {(forall A) (-> I32 A)}
  (<exit> status)
  (assert #f))

; TODO: argv
; TODO: command

(c-function time {(-> F64)} "llrt_time")

(c-function <exit> {(-> I32 unit)} "llrt_exit")
